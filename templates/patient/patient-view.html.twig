        <script>
            if (window.history.replaceState) {
                window.history.replaceState(null, null, window.location.href);
            }
            window.onload = function () {
                document.querySelectorAll(".tab-pane").forEach(obj => obj.classList.remove("active"));
                let hash = window.location.hash;
                let pane = hash ? document.querySelector(hash) : null
                if (pane !== null) {
                    pane.classList.add('active')
                } else {
                    document.querySelector("#timeline").classList.add('active')
                }
                let menu = document.querySelector("#menu-patient-list")

                menu.addEventListener('click', function (e) {
                    location.hash = e.target.hash
                })
            }
        </script>
